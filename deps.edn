{:paths ["src"]
 :deps {org.clojure/clojure                  {:mvn/version "1.12.0"}
        org.lwjgl/lwjgl                      {:mvn/version "3.3.5"}
        org.lwjgl/lwjgl$natives-linux        {:mvn/version "3.3.5"}
        org.lwjgl/lwjgl-assimp               {:mvn/version "3.3.5"}
        org.lwjgl/lwjgl-assimp$natives-linux {:mvn/version "3.3.5"}
        org.lwjgl/lwjgl-glfw                 {:mvn/version "3.3.5"}
        org.lwjgl/lwjgl-glfw$natives-linux   {:mvn/version "3.3.5"}
        org.lwjgl/lwjgl-openal               {:mvn/version "3.3.5"}
        org.lwjgl/lwjgl-openal$natives-linux {:mvn/version "3.3.5"}
        org.lwjgl/lwjgl-opengl               {:mvn/version "3.3.5"}
        org.lwjgl/lwjgl-opengl$natives-linux {:mvn/version "3.3.5"}
